    parent = main,
    title = "Aimbot Beta",
    text = "Auto Aim with Laser Cape or single web."
})

GUI:CreateToggle({
    parent = main, 
    text = "Toggle Me", 
    default = false, 
    callback = function(state)
        print("Toggle state:", state)
    end
})

